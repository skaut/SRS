{
    "name": "skaut/srs",

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/pecan987/nette-forms-gpspicker.git"
        }
    ],

    "require": {
        "php": ">=8.0",
        "ext-bcmath": "*",
        "ext-fileinfo": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-soap": "*",

        "nette/application": "^3.1",
        "nette/bootstrap": "^3.1",
        "nette/caching": "^3.1",
        "nette/database": "^3.1",
        "nette/di": "^3.0",
        "nette/finder": "^2.5",
        "nette/forms": "3.1.5",
        "nette/http": "^3.1",
        "nette/mail": "^3.1",
        "nette/robot-loader": "^3.4",
        "nette/security": "^3.1",
        "nette/utils": "^3.2",
        "latte/latte": "~2.10.0",
        "tracy/tracy": "~2.8.0",

        "contributte/console": "^0.9",
        "contributte/translation": "^1.0",

        "nettrine/orm": "^0.8",
        "nettrine/migrations": "^0.7",
        "beberlei/doctrineextensions": "^1.3",
        "dg/mysql-dump": "^1.5",

        "fmasa/messenger": "^2.0",

        "nextras/form-components": "^1.1",
        "nextras/forms-rendering": "^1.1",
        "vojtys/nette-forms-gpspicker": "dev-master",
        "contributte/recaptcha": "^3.4",

        "ublaboo/datagrid": "6.9.2",
        "ublaboo/mailing": "^1.2",

        "jms/serializer": "^3.17",
        "phpoffice/phpspreadsheet": "^1.21",
        "contributte/pdf": "^6.1",
        "mpdf/qrcode": "^1.2",
        "swejzi/qr-platba": "^1.0",
        "eluceo/ical": "^2.5",
        "endroid/qr-code": "^4.0",

        "skautis/skautis": "^2.2",
        "skautis/nette": "^2.2",

        "pear/numbers_words": "^0.18",
        "azuyalabs/yasumi": "^2.5",

        "mhujer/fio-api-php": "^4.1",
        "composer/ca-bundle": "^1.3"
    },

    "require-dev": {
        "nette/tester": "^2.4",
        "nextras/mail-panel": "^2.5",
        "codeception/codeception": "^4.1",
        "codeception/mockery-module": "^0.4",
        "contributte/codeception": "^1.3",
        "doctrine/coding-standard": "^9.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-nette": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.6",
        "rector/rector": "^0.12.20"
    },

    "autoload": {
        "classmap": ["app/Bootstrap.php"]
    },

    "autoload-dev": {
        "classmap": ["code-quality/"]
    },

    "minimum-stability": "stable",

    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
